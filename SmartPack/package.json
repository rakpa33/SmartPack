{
  "name": "smartpack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --exclude 'src/__tests__/integration/**'",
    "test:integration": "vitest run 'src/__tests__/integration/**/*.{test,spec}.{js,ts,jsx,tsx}'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:update": "playwright install --force",
    "lambda:dev": "node lambda/server.js",
    "dev:server": "nodemon lambda/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "lambda:build": "esbuild lambda/app.ts --bundle --platform=node --target=node20 --outfile=lambda/dist/app.js",
    "lambda:deploy": "cd lambda && serverless deploy",
    "lambda:test": "vitest --include lambda/app.test.ts",
    "ollama:test": "node lambda/test-ollama.js",
    "cleanup": "node scripts/cleanup-processes.js",
    "cleanup:verbose": "node scripts/cleanup-processes.js --verbose",
    "coordinator": "node scripts/run-coordinator.js",
    "coordinator:safe": "npm run cleanup && node scripts/run-coordinator.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "minimatch": "^10.0.3",
    "ollama": "^0.5.16",
    "playwright": "^1.54.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/deep-eql": "^4.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/prop-types": "^15.7.15",
    "@types/qs": "^6.14.0",
    "@types/range-parser": "^1.2.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/send": "^0.17.5",
    "@types/serve-static": "^1.15.8",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-router-dom": "^7.7.1",
    "serverless": "^4.0.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^14.4.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
